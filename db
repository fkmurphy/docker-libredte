--
-- Name: actividad_economica actividad_economica_pkey; Type: CONSTRAINT; Schema: public; Owner: prueba
--

ALTER TABLE ONLY public.actividad_economica
    ADD CONSTRAINT actividad_economica_pkey PRIMARY KEY (codigo);


--
-- Name: auth auth_pkey; Type: CONSTRAINT; Schema: public; Owner: prueba
--

ALTER TABLE ONLY public.auth
    ADD CONSTRAINT auth_pkey PRIMARY KEY (id);


--
-- Name: comuna comuna_pkey; Type: CONSTRAINT; Schema: public; Owner: prueba
--

ALTER TABLE ONLY public.comuna
    ADD CONSTRAINT comuna_pkey PRIMARY KEY (codigo);


--
-- Name: grupo grupo_pkey; Type: CONSTRAINT; Schema: public; Owner: prueba
--

ALTER TABLE ONLY public.grupo
    ADD CONSTRAINT grupo_pkey PRIMARY KEY (id);


--
-- Name: moneda_cambio moneda_cambio_pkey; Type: CONSTRAINT; Schema: public; Owner: prueba
--

ALTER TABLE ONLY public.moneda_cambio
